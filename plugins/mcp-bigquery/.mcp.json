{
  "mcpServers": {
    "bigquery": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-bigquery@latest",
        "--project",
        "${BIGQUERY_PROJECT}",
        "--location",
        "${BIGQUERY_LOCATION}",
        "--dataset",
        "${BIGQUERY_DATASET}",
        "--key-file",
        "${BIGQUERY_KEY_FILE}"
      ],
      "env": {},
      "envFile": "${workspaceFolder}/.env"
    }
  }
}
